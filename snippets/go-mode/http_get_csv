# name: http_get_csv
# key: http_get_csv
# --
resp, err := ${1:HTTPGeterFunc}(${2:url})
if err != nil {
    return err
}
defer resp.Body.Close()
csvReader := csv.NewReader(resp.Body)
for {
    record, err := csvReader.Read()
    if err == io.EOF {
        break
    }
    if err != nil {
        return err
    }
    // do something
}